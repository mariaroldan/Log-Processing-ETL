    <connection>metrics mysql</connection>
    <sql>SELECT CONCAT(date, &apos;-&apos;, time) AS timestamp
FROM dates d
CROSS JOIN (
	SELECT DISTINCT LEFT(time,2) AS time
	FROM times
	) t
WHERE CONCAT(date, &apos;-&apos;, time) BETWEEN ? AND ?
ORDER BY timestamp</sql>
