  <step>
    <name>Get Variables</name>
    <type>GetVariable</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <fields>
      <field>
        <name>datacenter</name>
        <variable>${DATACENTER}</variable>
        <type>String</type>
        <format/>
        <currency/>
        <decimal/>
        <group/>
        <length>-1</length>
        <precision>-1</precision>
        <trim_type>none</trim_type>
      </field>
      <field>
        <name>site</name>
        <variable>${SITE}</variable>
        <type>String</type>
        <format/>
        <currency/>
        <decimal/>
        <group/>
        <length>-1</length>
        <precision>-1</precision>
        <trim_type>none</trim_type>
      </field>
      <field>
        <name>server</name>
        <variable>${SERVER}</variable>
        <type>String</type>
        <format/>
        <currency/>
        <decimal/>
        <group/>
        <length>-1</length>
        <precision>-1</precision>
        <trim_type>none</trim_type>
      </field>
    </fields>
     <cluster_schema/>
 <remotesteps>   <input>   </input>   <output>   </output> </remotesteps>    <GUI>
      <xloc>50</xloc>
      <yloc>150</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

  <step>
    <name>Extract timestamp</name>
    <type>Janino</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
       <formula><field_name>timestamp</field_name>
<formula_string>filename.replaceFirst(&quot;.+?(20\\d{2}(?:-\\d{2}){3}).*&quot;, &quot;\\1&quot;)</formula_string>
<value_type>String</value_type>
<value_length>13</value_length>
<value_precision>-1</value_precision>
<replace_field/>
</formula>
     <cluster_schema/>
 <remotesteps>   <input>   </input>   <output>   </output> </remotesteps>    <GUI>
      <xloc>50</xloc>
      <yloc>250</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

  <step>
    <name>Log filenames</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <compatible>N</compatible>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>ConcatFilenames</jsScript_name>
        <jsScript_script>stringToLog += rpad(datacenter, &quot; &quot;, 3) + &quot; - &quot; + rpad(site, &quot; &quot;, 10) + &quot; - &quot; + rpad(server, &quot; &quot;, 20) + &quot; - &quot; + timestamp + &quot; - &quot; + rpad(filename, &quot; &quot;, 122) + &quot; - &quot; + lpad(size, &quot; &quot;, 10) + &quot;\n&quot;;</jsScript_script>
      </jsScript>      <jsScript>        <jsScript_type>1</jsScript_type>
        <jsScript_name>InitVariable</jsScript_name>
        <jsScript_script>var stringToLog = &quot;\n&quot;;</jsScript_script>
      </jsScript>      <jsScript>        <jsScript_type>2</jsScript_type>
        <jsScript_name>LogOutput</jsScript_name>
        <jsScript_script>writeToLog(&quot;m&quot;,stringToLog);</jsScript_script>
      </jsScript>    </jsScripts>    <fields>    </fields>     <cluster_schema/>
 <remotesteps>   <input>   </input>   <output>   </output> </remotesteps>    <GUI>
      <xloc>50</xloc>
      <yloc>350</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

