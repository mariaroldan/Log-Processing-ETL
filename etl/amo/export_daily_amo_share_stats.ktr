    <connection>metrics mysql</connection>
    <sql>SELECT addon_id
, COUNT(1) AS count
, service
, date
FROM f_addon_share_requests a
JOIN dates d ON a.utc_date_id = d.date_id
WHERE date ${DATE}
GROUP BY addon_id, service, date
</sql>
